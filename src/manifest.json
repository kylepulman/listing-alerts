{
  "manifest_version": 3,
  "name": "Listing Alerts",
  "version": "1.0",
  "icons": {
    "16": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": ["storage", "activeTab", "tabs", "scripting", "notifications", "alarms"],
  "host_permissions": ["https://*/*"]
}
